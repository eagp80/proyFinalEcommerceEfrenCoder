<div>
  <h1>Inicio de Sesion</h1>

  <div>
    <form action="/api/v1/session/login" method="post">
      <label for="email">Correo</label><br />
      <input
        type="email"
        name="email"
        id="email"
        placeholder="introduzca su correo electronico"
      />
      <br />
      <label for="password">Contrasena</label>
      <br />
      <input
        type="password"
        name="password"
        id="password"
        placeholder="introduzca su contrasena"
        required
      />
      <br />
      <button type="submit">submit</button>
    </form>
    
    <div>
        <br />
        <a href="/api/v1/session/github"><button type="submit">Login with GITHUB</button></a>
    </div>

    <form action="/api/v1/register" method="get">
        <br />
           <label for="submit">Ir a formulario de registro</label>      
        <br />
        <button type="submit">Go to register</button>
    </form>
    <form action="/api/v1/recover" method="get">
        <br />
           <label for="submit">Ir a formulario de restauración de contraseña</label>      
        <br />
        <button type="submit">Go to recover password</button>
    </form>
    
  </div>
</div>